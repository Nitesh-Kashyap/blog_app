<div id="<%= dom_id blog %>">
  <p>
    <strong>Name:</strong>
    <%= blog.name %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= blog.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= blog.content %>
  </p>

  <p>
    <strong>Created By:</strong>
    <%= blog.user.try(:email) %>
  </p>

</div>
